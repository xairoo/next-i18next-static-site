{
  "name": "next-i18next-static-site",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^3.0.1",
    "turbo": "^1.10.12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@8.19.1"
}
